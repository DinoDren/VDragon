local function BCNOEF_fake_script() -- WelcomeButton.LocalScript 
	local script = Instance.new('LocalScript', WelcomeButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.CreditsF.Visible = false
		script.Parent.Parent.Parent.ExtraF.Visible = false
		script.Parent.Parent.Parent.LocalF.Visible = false
		script.Parent.Parent.Parent.LocationsF.Visible = false
		script.Parent.Parent.Parent.WelcomeF.Visible = true
	end)
	
	
end
